{
    "coc.preferences.formatOnSaveFiletypes": [
        "Markdown",
        "coc-cfn-lint",
        "css",
        "go",
        "gomod",
        "html",
        "javascript",
        "javascriptreact",
        "json",
        "markdown",
        "markdownlint",
        "python",
        "ruby",
        "sql",
        "terraform",
        "typescript",
        "typescript.tsx",
        "typescriptreact",
        "yaml"
    ],
    "diagnostic.checkCurrentLine": true,
    "diagnostic.errorSign": ">>",
    "diagnostic.refreshAfterSave": true,
    "diagnostic.warningSign": "--",
    "inlayHint.enableParameter": false,
    "git.addedSign.text": "++",
    "git.branchCharacter": ".\ue0a0",
    "git.changeRemovedSign.text": "\u2243\u2243",
    "git.changedDecorator": "**",
    "git.changedSign.text": "~~",
    "git.conflictedDecorator": "xx",
    "git.removedSign.text": "__",
    "git.stagedDecorator": "\u25cf\u25cf",
    "git.topRemovedSign.text": "\u203e\u203e",
    "git.untrackedDecorator": "\u2026\u2026",
    "languageserver": {
        "digestif": {
            "command": "digestif",
            "filetypes": [
                "tex",
                "plaintex",
                "context"
            ]
        },
    },
    "markdownlint.onChange": false,
    "markdownlint.onOpen": false,
    "markdownlint.onSave": true,
    "lua.enable": false,
    "Lua.telemetry.enable": false,
    "Lua.workspace.library": [
        "${3rd}/luv/library"
    ],
    "sumneko-lua.enableNvimLuaDev": true,
    "workspace.ignoredFolders": [
        "$HOME",
        "$HOME/.cargo/**",
        "$HOME/.rustup/**"
    ]
}
